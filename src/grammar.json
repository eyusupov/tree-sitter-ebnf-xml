{
  "name": "ebnf",
  "rules": {
    "grammar": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "rule"
              },
              {
                "type": "REPEAT1",
                "content": {
                  "type": "STRING",
                  "value": "\n"
                }
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "rule"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "label"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": "::="
        },
        {
          "type": "SYMBOL",
          "name": "body"
        }
      ]
    },
    "label": {
      "type": "PATTERN",
      "value": "\\[[0-9A-Za-z]+\\]"
    },
    "body": {
      "type": "SYMBOL",
      "name": "expression"
    },
    "name": {
      "type": "SYMBOL",
      "name": "symbol"
    },
    "symbol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_start_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "_symbol"
        }
      ]
    },
    "_start_symbol": {
      "type": "PATTERN",
      "value": "\\p{Uppercase}(\\w|_)*"
    },
    "_symbol": {
      "type": "PATTERN",
      "value": "\\p{Lowercase}(\\w|_)*"
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "rule_ref"
        },
        {
          "type": "SYMBOL",
          "name": "hex_char"
        },
        {
          "type": "SYMBOL",
          "name": "char_match"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "unit"
        },
        {
          "type": "SYMBOL",
          "name": "optional"
        },
        {
          "type": "SYMBOL",
          "name": "sequence"
        },
        {
          "type": "SYMBOL",
          "name": "alternation"
        },
        {
          "type": "SYMBOL",
          "name": "difference"
        },
        {
          "type": "SYMBOL",
          "name": "one_or_more"
        },
        {
          "type": "SYMBOL",
          "name": "zero_or_more"
        }
      ]
    },
    "rule_ref": {
      "type": "SYMBOL",
      "name": "symbol"
    },
    "hex_char": {
      "type": "SYMBOL",
      "name": "hex_integer"
    },
    "hex_integer": {
      "type": "PATTERN",
      "value": "#x[0-9a-fA-F]+"
    },
    "char_match": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "neg_marker"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_char_atom"
              },
              {
                "type": "SYMBOL",
                "name": "range"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "neg_marker": {
      "type": "STRING",
      "value": "^"
    },
    "match_character": {
      "type": "PATTERN",
      "value": "[^\\]-]"
    },
    "range": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_char_atom"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "_char_atom"
        }
      ]
    },
    "_char_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "hex_integer"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "STRING",
                "value": "-"
              },
              "named": true,
              "value": "match_character"
            },
            {
              "type": "SYMBOL",
              "name": "match_character"
            }
          ]
        }
      ]
    },
    "unit": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_string"
        }
      ]
    },
    "single_quoted_string": {
      "type": "PATTERN",
      "value": "'[^']*'"
    },
    "double_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "\"([^\"]|(\\\\\\\\\"))*\""
          },
          {
            "type": "PATTERN",
            "value": "\"\\\\\"\""
          }
        ]
      }
    },
    "optional": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "STRING",
            "value": "?"
          }
        ]
      }
    },
    "sequence": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "_expression_opt_nl": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "alternation": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_expression_opt_nl"
          },
          {
            "type": "STRING",
            "value": "|"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "difference": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "PREC_LEFT",
        "value": 0,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "SYMBOL",
              "name": "expression"
            }
          ]
        }
      }
    },
    "one_or_more": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "STRING",
            "value": "+"
          }
        ]
      }
    },
    "zero_or_more": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "STRING",
            "value": "*"
          }
        ]
      }
    },
    "comments": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "."
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "wfc": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[ wfc:"
        },
        {
          "type": "SYMBOL",
          "name": "constraint_name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "vc": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[ vc:"
        },
        {
          "type": "SYMBOL",
          "name": "constraint_name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "constraint_name": {
      "type": "PATTERN",
      "value": "[^\\]]"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comments"
    }
  ],
  "conflicts": [
    [
      "char_match",
      "range"
    ],
    [
      "body",
      "_expression_opt_nl"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
