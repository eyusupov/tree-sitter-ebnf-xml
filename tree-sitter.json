{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ebnfxml",
      "camelcase": "EbnfXml",
      "title": "EBNF XML",
      "scope": "source.ebnfxml",
      "file-types": [
        "ebnf"
      ],
      "injection-regex": "^ebnfxml$",
      "class-name": "TreeSitterEbnfxml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for EBNF variant as described in XML spec",
    "authors": [
      {
        "name": "Eldar Yusupov",
        "email": "eyusupov@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/eyusupov/tree-sitter-ebnf-xml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}